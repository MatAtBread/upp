#include "io-lite.h"

@include(method.hup)

typedef struct {
    int x;
    int y;
} Point;

@method(Point) int distance(Point * p) {
    return p->x * p->x + p->y * p->y;
}

int main() {
    Point p;
    p.x = 10;
    p.y = 20;
    int r = p.distance();
    printf("distance: %d\n", r);

    Point * q = &p;
    int r2 = q->distance();
    printf("distance: %d\n", r2);
    return 0;
}
