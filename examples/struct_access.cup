#include "io-lite.h"

@include(method.hup)

struct Point {
    int x;
    int y;
};
typedef struct Point Point;

@method(Point) int distance(Point * p) {
    return p -> x * p -> x + p -> y * p -> y;
}

int main() {
    Point p = { 3, 4};
    int r = p.distance();
    printf("distance: %d\n", r);
    return 0;
}
