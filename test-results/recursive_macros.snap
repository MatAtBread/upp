==== examples/recursive_macros.c ===
/* A minimal C stdio/stdlib for upp to make it easy to
   see the output of the examples but permit compilation
*/
extern int puts(const char *s);
extern int printf(const char *format, ...);
extern int fputs(const char *s, void *stream);
extern void *malloc(unsigned long n);
extern void free(void *p);
extern char *strcpy(char *dest, const char *src);
extern void *stderr;
extern void *_stderr;
#include "method.h"
#include "defer.h"
int some_condition = 1;
struct String {
    char *data;
};
typedef struct String String;
 void _String_method_print(String *s) {
    printf("%s\n", s->data);
}
 void _String_method_Defer(String *s) {
    s->print();
    free(s->data);
}
int main() {
    String s1;
    s1.data = malloc(100);
    strcpy(s1.data, "Hello");
    if (some_condition) {
        return 0;
    }
    return 1;
}

