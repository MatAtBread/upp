==== examples/trace_3.c ===
#include "trace.h"
static int _trace_my_function_1(int x, char *y) {
    int g = 1;
    for (int i = 1; i < x; i++) {
        g = g * i;
    }
    return g;
}

int my_function(int x,char *y) {
    printf(">> my_function (x = %d, y = %s)\n", x, y);
    int r = _trace_my_function_1(x, y);
    printf("<< my_function = %d\n", r);
    return r;
} 
int main() {
    printf("magic number %d\n", my_function(10,"Foo"));
    return 0;
}

