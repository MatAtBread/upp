#include "method.h"
struct Point {
    int x;
    int y;
};
typedef struct Point Point;
// Standardized paren style.
 int _Point_method_distance(Point *p) {
    return p->x * p->x + p->y * p->y;
}
int main() {
    Point p;
    int r = p.distance();
    return 0;
}

