==== examples/match_longify.c ===
/* A minimal C stdio/stdlib for upp to make it easy to
   see the output of the examples but permit compilation
*/
extern int puts(const char *s);
extern int printf(const char *format, ...);
extern int fputs(const char *s, void *stream);
extern void *malloc(unsigned long n);
extern void free(void *p);
extern char *strcpy(char *dest, const char *src);
extern void *stderr;
extern void *_stderr;


/*@longify(bar)*/
/*@longify(baz)*/
int foo = 100;
long bar = (long)(200);
long baz = (long)(5);
void f() {
    long bar = (long)(4);
}
void global() {
    printf("global\n");
    printf("bar is of type " "long" "\nbaz is of type " "long" "\n");
}
int main() {
    long bar = (long)(5);
    printf("main\n");
    printf("bar is of type %s\nbaz is of type %s\n", "long", "long");
    global();
    return 0;
}

==== RUN OUTPUT ===
main
bar is of type long
baz is of type long
global
bar is of type long
baz is of type long

